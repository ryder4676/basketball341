{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "no-unused-vars": "warn",
        "no-console":"off",
        "prettier/prettier": "error",
        "func-names": "off",
        "no-process-exit":"off",
        "object-shorthand":"off"

    }
}